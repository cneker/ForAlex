<Window x:Class="lab04.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab04.Views"
        mc:Ignorable="d"
        Title="lab04" Height="350" Width="300">
    <StackPanel HorizontalAlignment="Center" Width="175" >
        <StackPanel.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="Margin" Value="0,20,0,0"/>
            </Style>
        </StackPanel.Resources>
        <Button Content="Employees" Height="30" Click="Employees"/>
        <Button Content="Items" Height="30" Click="Items"/>
        <Button Content="Organizations" Height="30" Click="Organizations"/>
        <Button Content="Positions" Height="30" Click="Positions"/>
        <Button Content="Orders" Height="30" Click="Orders"/>

    </StackPanel>
</Window>
